@model IEnumerable<Project.Models.Autenticacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StrContrasena)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StrContrasena)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StrUsuario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StrUsuario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StrUsuario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
